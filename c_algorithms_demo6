#include <stdio.h>
#include <math.h>
#include <time.h>
#include <string.h>
#include <ctype.h>
#include <assert.h>
int main()
{  
   int temp,a=3,b=4;
   printf("%d --  %d\n",a,b);
   temp=a;
   a=b;
   b=temp;
   printf("%d --  %d\n",a,b);
   a=b+a;
   b=a-b;
   a=a-b; 
   printf("%d --  %d\n",a,b);
   return 0;
}
